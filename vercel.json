{
    "version": 2,
    "name": "iraq-election-frontend",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "env": {
        "VITE_API_URL": "https://iraq-election-backend.onrender.com",
        "NEXT_PUBLIC_API_URL": "https://iraq-election-backend.onrender.com"
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://iraq-election-backend.onrender.com/api/:path*"
        }
    ],
    "framework": "vite",
    "regions": [
        "fra1"
    ],
    "outputDirectory": "dist"
}